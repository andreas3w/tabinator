{
  "manifest_version": 3,
  "name": "Tabinator",
  "version": "1.0.0",
  "description": "Limits unpinned tabs and asks what to close before opening more.",
  "permissions": ["tabs", "storage"],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Tabinator",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png"
    },
    "default_popup": "popup.html"
  }
}
